<?xml version="1.0" encoding="UTF-8"?>
	<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"> 
		<url>
				<loc>{{host}}</loc>
		</url>
		{# <url>
				<loc>{{host}}/ploshhadki/</loc>
		</url> #}
		{# <url>
				<loc>{{host}}/contacts/</loc>
		</url> #}

		{% if main_subdomain %}
				<url>
					<loc>{{host}}/blog/</loc>
				</url>
				{% for post in blog %}
					<url>
						<loc>{{host}}/blog/{{post.alias}}/</loc>
					</url>
				{% endfor %}
		{% endif %}

		<url>
				<loc>{{host}}/politika-konfidentsialnosti/</loc>
		</url>

		{% for slice in slices %} 
			<url>
				<loc>{{host}}/{{slice.alias}}/</loc> 
			</url>
		{% endfor %}

		{% for item in items %}
			<url>
				<loc>{{host}}/banket-{{item.restaurant_slug}}/</loc>
			</url>
		{% endfor %}
		
	</urlset>